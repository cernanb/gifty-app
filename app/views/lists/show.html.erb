<% if @list.id == current_user.id %>
  <h1> This is my wish list </h1>


  <p><%= form_for([@list, @item]) do |f| %>
    Add Items  <%= f.text_field :name %>
      <%= f.submit style: "display: none" %>
  <% end %></p>
<% else %>
  <h1> "<%= @list.user.username%>'s Wishlist" </h1>
<% end %>

<br>

  <% @list.items.each do |item| %>
    <h2><%= item.name %></h2>
  <% end %>
